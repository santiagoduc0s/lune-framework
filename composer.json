{
  "name": "santiducos/lune",
  "description": "Simple php framework",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lune\\": "src/"
    }
  },
  "scripts": {
    "serve": "php -S localhost:8000 -t ./public",
    "tests": "phpunit --color=always --testdox -v tests/",
    "php-cs-fixer": ".\\vendor\\bin\\php-cs-fixer fix --diff"
  },
  "autoload-dev": {
    "psr-4": {
      "Lune\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Santiago Ducos",
      "email": "santidu200@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.11"
  }
}
